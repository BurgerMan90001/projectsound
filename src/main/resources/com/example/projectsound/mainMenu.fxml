<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="412.0" prefWidth="639.0" spacing="20.0" style="-fx-background-color: #13315C;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.projectsound.SceneController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <AnchorPane prefHeight="318.0" prefWidth="341.0" style="-fx-background-color: #0B2545;">
      <children>
          <Button fx:id="myButton" contentDisplay="TOP" layoutX="147.0" layoutY="242.0" onAction="#displayImage" prefHeight="27.0" prefWidth="114.0" text="HELP ME !!!!" textOverrun="WORD_ELLIPSIS" />
         <Button fx:id="artistButton" layoutX="300.0" layoutY="238.0" mnemonicParsing="false" onAction="#switchToArtistPage" prefHeight="36.0" prefWidth="185.0" text="Click To Switch To Artist Page" />
         <ImageView fx:id="myImageView" fitHeight="168.0" fitWidth="244.0" layoutX="63.0" layoutY="31.0" onMouseClicked="#displayImage" pickOnBounds="true" preserveRatio="true">
            <image>
               <Image url="@dake.jpg" />
            </image>
            <cursor>
               <Cursor fx:constant="OPEN_HAND" />
            </cursor>
         </ImageView>
      </children>
   </AnchorPane>
</VBox>
